{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.meta": true,
    "**/*.fbx": true,
    "Cache/": true,
    "Libraries/": true
  },
  "workbench.colorTheme": "Visual Studio Dark - C++",
  "editor.semanticTokenColorCustomizations": {
    "rules": {
      "*.deprecated": {
        "fontStyle": "strikethrough"
      }
    }
  },
  "path-intellisense.extensionOnImport": true
}
